<!DOCTYPE html>
<html>
    <head>
          <% include ../partials/head %>
    <script type="text/javascript" src="/js/jquery.min.js"></script>
    <script type="text/javascript" src="/js/accordion.js"></script> 

        
    </head>
    <body class="appear-animate">
        
        <!-- Page Loader -->        
        <% include ../partials/loader %>
        <!-- End Page Loader -->
        
        <!-- Page Wrap -->
        <div class="page" id="top">
            
            <!-- Navigation panel -->
            <!-- End Navigation panel -->
            
            
            <!-- Head Section -->
            <section class="page-section bg-dark-alfa-30 parallax-3" data-background="images/full-width-images/testtube.jpg">
                <div class="relative container align-left">
                    
                    <div class="row">
                        
                        <div class="col-md-8">
                            <h1 class="hs-line-11 font-alt mb-20 mb-xs-0"><%= biomarkerInfo['Name'] %></h1>
                            <div class="hs-line-4 font-alt">
                                Alternate names: <% biomarkerInfo['Alternate Names'].forEach(function(name) { %>
                                    <%= name %>,
                                <% }); %>
                            </div>
                        </div>
                        
                     
                    </div>
                    
                </div>
            </section>
            <!-- End Head Section -->
            <section class="page-section">
                <div class="container relative">
                    
                   
                    
                    <!-- Row -->
                    <div class="row">
                        
                        <!-- Col -->
                        
                        <div class="col-sm-10 col-sm-offset-1">
                            
                       
                            <!-- End Nav Tabs -->
                            
                            <!-- Tab panes -->
                            <div class="col-sm-6 mb-xs-40">
                            
                            <!-- Accordion -->
                            <dl class="toggle">
                                <dt>
                                    <a href="" class="">Type</a>
                                </dt>
                                <dd style="display: none;">
                                    <%= biomarkerInfo['Type'] %>
                                </dd>
                                <dt>
                                    <a href="">Medium</a>
                                </dt>
                                <dd style="display: none;">
                                <%= biomarkerInfo['Medium'] %>

                                </dd>

                                <dt>
                                    <a href="" class="active">Sources</a>
                                </dt>
                                <dd style="display: none;">
                                                             <% for(var i=0; i < biomarkerInfo['Sources'].length; i++) { %>

                                      <a href='<%= biomarkerInfo['Sources'][i]['URL'] %>' target="_blank">
                                        <%= biomarkerInfo['Sources'][i]['Google ID'] %>
                                    </a>
                                    &nbsp;(<%= biomarkerInfo['Sources'][i]['Source Type'] %> from <%= biomarkerInfo['Sources'][i]['Original Source Database'] %>)
                                                                  <% } %>  

                                </dd>
                                  <dt>
                                    <a href="" class="active">Drug Associations</a>
                                </dt>
                                <dd style = "display:none;">
                                 <% for(var i=0; i < biomarkerInfo['Sources'].length; i++) { %>

                                     <% if (biomarkerInfo['Sources'][i]['Drug Name'] !== null) { %>
                                    <p class="dossier_label">Drug Name:&nbsp;&nbsp;</p>
                                    <%= biomarkerInfo['Sources'][i]['Drug Name'] %>
                                <% } else{ %>
                                None
                                 <% }} %>  
                                </dd>
                            </dl>
                            <!-- End Accordion -->
                            
                        </div>
                        </div>
                        
                        <!-- End Col -->                        
                        
                    </div>
                    <!-- End Row -->
                    
                </div>
            </section>
            
          
                    <!-- Footer Text -->
                        <% include ../partials/footer %>

            <!-- End Foter -->
        
        
        </div>
        <!-- End Page Wrap -->
        
        
        <!-- JS -->
        <script type="text/javascript" src="js/jquery-1.11.2.min.js"></script>
        <script type="text/javascript" src="js/jquery.easing.1.3.js"></script>
        <script type="text/javascript" src="js/bootstrap.min.js"></script>        
        <script type="text/javascript" src="js/SmoothScroll.js"></script>
        <script type="text/javascript" src="js/jquery.scrollTo.min.js"></script>
        <script type="text/javascript" src="js/jquery.localScroll.min.js"></script>
        <script type="text/javascript" src="js/jquery.viewport.mini.js"></script>
        <script type="text/javascript" src="js/jquery.countTo.js"></script>
        <script type="text/javascript" src="js/jquery.appear.js"></script>
        <script type="text/javascript" src="js/jquery.sticky.js"></script>
        <script type="text/javascript" src="js/jquery.parallax-1.1.3.js"></script>
        <script type="text/javascript" src="js/jquery.fitvids.js"></script>
        <script type="text/javascript" src="js/owl.carousel.min.js"></script>
        <script type="text/javascript" src="js/isotope.pkgd.min.js"></script>
        <script type="text/javascript" src="js/imagesloaded.pkgd.min.js"></script>
        <script type="text/javascript" src="js/jquery.magnific-popup.min.js"></script>
        <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&amp;language=en"></script>
        <script type="text/javascript" src="js/gmap3.min.js"></script>
        <script type="text/javascript" src="js/wow.min.js"></script>
        <script type="text/javascript" src="js/masonry.pkgd.min.js"></script>
        <script type="text/javascript" src="js/jquery.simple-text-rotator.min.js"></script>
        <script type="text/javascript" src="js/all.js"></script>
        <script type="text/javascript" src="js/contact-form.js"></script>
        <script type="text/javascript" src="js/jquery.ajaxchimp.min.js"></script>        
        <!--[if lt IE 10]><script type="text/javascript" src="js/placeholder.js"></script><![endif]-->
        
    </body>
        <% if(logged_in) { %>
<% include ../partials/navbar_loggedin %>
<% } else { %>
<% include ../partials/navbar %>
<% } %>
</html>
